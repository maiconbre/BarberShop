{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:local": "vite --mode development",
    "dev:prod": "vite --mode production",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "preview": "vite preview",
    "preview:dev": "vite preview --mode development",
    "preview:prod": "vite preview --mode production",
    "test:supabase": "ts-node src/scripts/test-supabase.ts",
    "start:dev": "powershell -File ./start-dev.ps1",
    "env:switch-local": "copy .env.development .env",
    "env:switch-prod": "copy .env.production .env",
    "prisma": {
      "seed": "node --loader ts-node/esm prisma/seed.ts"
    },
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "@shadcn/ui": "^0.0.4",
    "@supabase/supabase-js": "^2.48.1",
    "@types/recharts": "^1.8.29",
    "axios": "^1.8.1",
    "chart.js": "^4.4.8",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.4.9",
    "lucide-react": "^0.344.0",
    "next-themes": "^0.4.4",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-lazy-load-image-component": "^1.6.3",
    "react-router-dom": "^7.1.5",
    "react-tooltip": "^5.28.0",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "recharts": "^2.15.1",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7",
    "zod": "^4.0.5",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "bcryptjs": "^3.0.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.4",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "prisma": "^6.4.1",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.1",
    "terser": "^5.39.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^6.1.1",
    "vite-plugin-compression2": "^1.3.3",
    "vitest": "^3.2.4"
  }
}
