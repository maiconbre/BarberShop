{
    "version": 2,
    "framework": "vite",
    "routes": [
        {
            "src": "/assets/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "continue": true
        },
        {
            "src": "/(.*).(ico|png|jpg|jpeg|svg|webp)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "continue": true
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}